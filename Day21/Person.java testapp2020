package com.example.testapp2020;

public class Person {

    private String name;
    private String surname;

    //constructor
    public Person() {

        name = null;
        surname = null;
    }

    //getters and setters
    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getSurname() {
        return surname;
    }

    public void setSurname(String surname) {
        this.surname = surname;
    }
}
